<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<!-- Application infos -->
	<application>igotaloop</application>
	<version>1.1</version>
	
	<!--  Sessions enabled -->
	<sessions-enabled>true</sessions-enabled>
	
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	
	<!-- Static files -->
	<static-files>
        <include path="/**.png" expiration="20d 5h" />
        <include path="/**.gif" expiration="20d 5h" />
        <include path="/**.ico" expiration="20d 5h" />
        <include path="/**.jpg" expiration="20d 5h" />
        <include path="/**.js"  expiration="20d 5h" />
        <include path="/**.css" expiration="20d 5h" />
    </static-files>

</appengine-web-app>