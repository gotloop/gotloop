{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": false,
    "removeComments": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "cucumber"],
    "lib": ["es2015"]
  },
  "exclude": ["node_modules", "typescript"]
}
