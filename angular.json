{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "www": {
      "root": "apps/www",
      "sourceRoot": "apps/www/src",
      "projectType": "application",
      "prefix": "glp",
      "schematics": {
        "@ngrx/schematics:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/www",
            "index": "apps/www/src/index.html",
            "main": "apps/www/src/main.ts",
            "polyfills": "apps/www/src/polyfills.ts",
            "tsConfig": "apps/www/tsconfig.app.json",
            "preserveSymlinks": true,
            "assets": [
              "apps/www/src/favicon.ico",
              "apps/www/src/assets"
            ],
            "styles": [
              "apps/www/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/www/src/",
                "apps/www/src/styles",
                "node_modules/"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/www/src/environments/environment.ts",
                  "with": "apps/www/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "apps/www/src/environments/environment.ts",
                  "with": "apps/www/src/environments/environment.hmr.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "www:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "www:build:production"
            },
            "hmr": {
              "hmr": true,
              "browserTarget": "www:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "www:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/www/src/test.ts",
            "polyfills": "apps/www/src/polyfills.ts",
            "tsConfig": "apps/www/tsconfig.spec.json",
            "karmaConfig": "apps/www/karma.conf.js",
            "styles": [
              "apps/www/src/styles.scss"
            ],
            "assets": [
              "apps/www/src/favicon.ico",
              "apps/www/src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/sass"
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/www/tsconfig.app.json",
              "apps/www/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "www-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "www:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "api-model": {
      "root": "libs/api-model",
      "sourceRoot": "libs/api-model/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/api-model/tsconfig.lib.json",
              "libs/api-model/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/api-model/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-model/jest.config.js",
            "tsConfig": "libs/api-model/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "defaultProject": "www",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}